<cleaner id="irfanview" os="windows">
  <label>IrfanView</label>
  <description>Picture viewer</description>
  <!-- To reduce the lines we need, lets work with a variable for the filename. Tobias. -->
  <var name="Filename">
    <!-- 32-bit version of IrfanView: -->
    <value>i_view32.ini</value>
    <!-- 64-bit version of IrfanView: -->
    <value>i_view64.ini</value>
  </var>
  <!-- That we don't need so much translation, we combine all in "History"! Tobias. -->
  <option id="history">
    <label>History</label>
    <description>Delete the usage history</description>
    <!-- That we don't need so much translation, we combine all in "History"! Tobias.
  <option id="open_opendir">
    <label>Open Directory</label>
    <description>Directory shown in the open dialog</description>
  -->
    <action command="ini" search="file" path="%AppData%\IrfanView\$$Filename$$" section="Open" parameter="OpenDir"/>
  <!-- That we don't need so much translation, we combine all in "History"! Tobias.
  </option>
  <option id="thumbnails_mrud">
    <label>MRU - Thumbnails</label>
    <description>Most recent used (MRU) - Thumbnails Directories</description>
  -->
    <action command="ini" search="file" path="%AppData%\IrfanView\$$Filename$$" section="Thumbnails" parameter="MRUD1"/>
