<cleaner id="regedit" os="windows">
  <label>Registry Editor</label>
  <description>Utility</description>
  <!-- That we don't need so much translation, we combine all in "History"! Tobias. -->
  <option id="history">
    <label>History</label>
    <description>Delete the usage history</description>
    <!-- That we don't need so much translation, we combine all in "History"! Tobias.
  <option id="lastkey">
    <label>Last Key</label>
    <description>Delete the Last Key Setting</description>
  -->
    <action command="winreg" path="HKCU\Software\Microsoft\Windows\CurrentVersion\Applets\Regedit" name="LastKey"/>
  </option>
</cleaner>
