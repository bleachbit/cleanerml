<?xml version="1.0" encoding="UTF-8"?>
<!--
  @app            ClickOnce
  @os             Windows
  @cleanerversion 0.0.1
  @cleanerdate    2025-10-09
  @testeddate     2025-10-09
  @note Info source: https://learn.microsoft.com/en-us/previous-versions/troubleshoot/dotnet/framework/clickonce-application-fails-update
  @note To delete the ClickOnce file cache, delete the contents of this folder based on the operating system.
  @note   Windows XP    and Server 2003           - %userprofile%\Local Settings\Apps\2.0\*.*
  @note   Windows Vista and Server 2008 and later - %userprofile%\AppData\Local\Apps\2.0\*.*
-->
<cleaner id="Cache_ClickOnce" os="windows"><label>ClickOnce</label>
  <option id="cache"><label>Cache</label>
    <description>Clear ClickOnce file cache</description>
    <action command="delete" search="walk.all" path="%LOCALAPPDATA%/Apps/2.0"/>
    <!-- walk.all: match all file/dirs under a dir (but not the top dir)
    %LOCALAPPDATA% = C:\Users\USER_NAME\AppData\Local -->
  </option>
</cleaner>
