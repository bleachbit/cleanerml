<cleaner id="akelpad" os="windows">
  <label>Akelpad</label>
  <description>Text editor</description>
  <option id="recent">
    <label>Recently accessed files</label>
    <description>Deletes recent files entries from File menu.</description>
    <action command="winreg" path="HKCU\Software\Akelsoft\AkelPad\Recent"/>
  </option>
  <option id="search">
    <label>Saved search terms</label>
    <description>Deletes terms from Search drop-down list.</description>
    <action command="winreg" path="HKCU\Software\Akelsoft\AkelPad\Search"/>
  </option>
  <option id="lastdir">
    <label>Last directory used</label>
    <description>Deletes directory path trace.</description>
    <action command="winreg" path="HKCU\Software\Akelsoft\AkelPad\Options" name="LastDirectory"/>
  </option>
</cleaner>
