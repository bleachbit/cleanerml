<cleaner id="gnome-art" os="linux">
  <label>GNOME Art</label>
  <option id="thumbs">
    <label>Thumbnails</label>
    <description>Delete thumbnails</description>
    <action command="delete" search="walk.files" path="~/.gnome2/gnome-art/thumbnails/"/>
  </option>
</cleaner>
